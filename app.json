{
  "expo": {
    "name": "SAP_Festival",
    "slug": "SAP_Festival",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "scheme": "myapp",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true,
      "backgroundColor": "#5a9adb"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/adaptive-icon.png",
        "backgroundColor": "#5a9adb"
      },
      "package": "com.guillaumebnd.SAP_Festival"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/splash-icon.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#5a9adb"
        }
      ],
      [
        "expo-font",
        {
          "fonts": ["./assets/fonts/Oliver-Regular.otf"]
        }
      ]      
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "f8c7969c-d2d2-4e76-a221-013bcee6990e"
      }
    },
    "owner": "guillaumebnd"
  }
}
